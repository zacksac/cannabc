{
  "components/cart/modals/configure-product-3": "{{#each options}}\n    {{{dynamicComponent 'components/halothemes/configure-options'}}}\n{{/each}}\n",
  "components/halothemes/configure-options/date": "<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n",
  "components/halothemes/configure-options/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{display_name}}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <input\n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"edit_attribute-check-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"edit_attribute-check-{{id}}\">{{label}}</label>\n</div>\n",
  "components/halothemes/configure-options/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\n\t<label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_file_{{id}}\">\n        {{ this.display_name }}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <input class=\"form-file\" type=\"file\" id=\"edit_attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n</div>\n",
  "components/halothemes/configure-options/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\n\t<label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_number_{{id}}\">\n        {{ this.display_name }}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <input class=\"form-input\" type=\"number\" id=\"edit_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/configure-options/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\n\t<label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_text_{{id}}\">\n        {{ this.display_name }}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"edit_attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/configure-options/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{display_name}}:\n        <span data-option-value></span>\n        {{> components/common/requireness-msg}}\n    </label>\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"edit_attribute_productlist_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"edit_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{#if ../theme_settings.halo_homepage_layout_1}}{{cdn 'img/loading.svg'}}{{else}}{{cdn 'img/loading_2.svg'}}{{/if}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"edit_attribute_productlist_{{../id}}_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"edit_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"edit_attribute_productlist_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"edit_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"edit_attribute_productlist_{{../id}}_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"edit_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/halothemes/configure-options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{ this.display_name }}:\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"edit_attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"edit_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n",
  "components/halothemes/configure-options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{this.display_name}}:\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"edit_attribute_rectangle_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-rectangle\" for=\"edit_attribute_rectangle_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            <span class=\"form-option-variant\"><span>{{this.label}}</span></span>\n        </label>\n    {{/each}}\n</div>\n",
  "components/halothemes/configure-options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_select_{{id}}\">\n        {{ this.display_name }}:\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"edit_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/halothemes/configure-options/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\n\t<label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_textarea_{{id}}\">\n        {{display_name}}:\n        {{> components/common/requireness-msg}}\n    </label>\n    <textarea class=\"form-input\" id=\"edit_attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}} {{#if max_length}}maxlength={{max_length}}{{/if}}>{{prefill}}</textarea>\n</div>\n",
  "components/halothemes/configure-options/swatch": "<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{this.display_name}}:\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"edit_attribute_swatch_{{../id}}_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch\" for=\"edit_attribute_swatch_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\n                    <span style=\"background-color: #{{data.[0]}}\"></span>\n                    <span style=\"background-color: #{{data.[1]}}\"></span>\n                    <span style=\"background-color: #{{data.[2]}}\"></span>\n                </span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\n                        <span style=\"background-color: #{{data.[0]}}\"></span>\n                        <span style=\"background-color: #{{data.[1]}}\"></span>\n                    </span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}*{{/if}}\n</small>\n"
}

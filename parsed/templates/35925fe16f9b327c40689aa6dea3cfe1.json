  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open {{display_name}} in a new tab\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{display_name}}\">\n                        <title>{{display_name}}</title>\n                        <desc>{{display_name}}</desc>\n                        <use xlink:href=\"#icon-{{display_name}}\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.social_icon_google_plus_link}}\n            <li class=\"socialLinks-item google-plus\">\n                <a class=\"icon icon--google-plus\" href=\"{{theme_settings.social_icon_google_plus_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Google Plus Social Media in a new tab\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"Google Plus\">\n                        <title>Google Plus</title>\n                        <desc>Google Plus</desc>\n                        <use xlink:href=\"#icon-google-plus\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_snapchat_link}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Snapchat Social Media in a new tab\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"Snapchat Ghost\">\n                        <title>Snapchat Ghost</title>\n                        <desc>Snapchat Ghost</desc>\n                        <use xlink:href=\"#icon-snapchat-ghost\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/if}}\n"
